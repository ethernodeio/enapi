{
  "extends": [
    "tslint:recommended"
  ],
  "rules": {
    "ordered-imports": false,
    "object-literal-sort-keys": false,
    "max-line-length": false,
    "prefer-const": false,
    "no-var-keyword": false,
    "variable-name": false,
    "no-console": [
      false
    ],
    "indent": [
      true,
      "spaces",
      2
    ],
    "max-classes-per-file": false
  }
}